    "passport-local": "^1.0.0",
    "passport-local-mongoose": "^4.2.1",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-autocomplete": "^1.7.2",
    "react-dom": "^15.6.1",
    "react-redux": "^5.0.5",
    "react-router": "^4.1.2",
    "react-router-dom": "^4.1.2",
    "redux": "^3.7.2",
    "redux-devtools-extension": "^2.13.2",

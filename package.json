{
  "name": "scrooge",
  "version": "0.1.0",
  "description": "An app to track your cryptocoins value",
  "main": "server/src/server.js",
  "engines": {
    "node": "8.0.0"
  },
  "scripts": {
    "build": "NODE_ENV=\"production\" webpack --progress",
    "dev": "nodemon --watch server/dist/ server/dist/server.js",
    "heroku-postbuild": "npm run build",
    "lint": "eslint",
    "precommit": "npm test",
    "start": "node server/dist/server.js",
    "test": "mocha --require babel-register ./client/src/**/__tests__/*-test.js ./server/src/**/__tests__/*-test.js",
    "watch-sources": "parallel-webpack --watch",
    "watch-sources:client": "webpack --watch --progress --config client/webpack.client.config.js",
    "watch-sources:server": "webpack --watch --progress --config server/webpack.server.config.js",
    "webpack": "webpack"
  },
  "author": "Gleb Kost",
  "license": "ISC",
  "devDependencies": {
    "chai": "^4.1.2",
    "eslint": "^4.14.0",
    "eslint-config-airbnb": "^15.1.0",

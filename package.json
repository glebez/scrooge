{
  "name": "scrooge",
  "version": "0.1.0",
  "description": "An app to track your cryptocoins value",
  "main": "index.js",
  "scripts": {
    "watch-sources": "node node_modules/.bin/parallel-webpack --watch",
    "watch-sources:client": "webpack --watch --progress --config client/webpack.client.config.js",
    "watch-sources:server": "webpack --watch --progress --config server/webpack.server.config.js",
    "build": "node node_modules/.bin/parallel-webpack",
    "start": "npm run build && node server/dist/server.js",
    "dev": "nodemon --watch server/dist/ server/dist/server.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Gleb Kost",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.1",
    "babel-plugin-syntax-async-functions": "^6.13.0",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-preset-airbnb": "^2.4.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "clean-webpack-plugin": "^0.1.16",
    "eslint": "^4.4.1",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.2.1",
    "file-loader": "^0.11.2",
    "html-webpack-plugin": "^2.30.1",
    "jsx-loader": "^0.13.2",
    "parallel-webpack": "^2.1.0",
    "redux-devtools": "^3.4.0",
    "webpack": "^3.4.1",
    "webpack-dev-server": "^2.6.1"
  },
  "dependencies": {
    "axios": "^0.16.2",
    "babel-polyfill": "^6.26.0",
    "dotenv": "^4.0.0",
    "express": "^4.15.4",
    "glamor": "^2.20.39",
    "glamorous": "^4.1.0",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-redux": "^5.0.5",
    "react-router": "^4.1.2",
    "react-router-dom": "^4.1.2",
    "redux": "^3.7.2",
